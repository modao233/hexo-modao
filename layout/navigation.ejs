<%- css('css/font.css') %>
<%- css('css/navigation.css') %>

<div class="page-container">
  <div class="sidebar-menu toggle-others fixed">
    <div class="sidebar-menu-inner ps-container">
	  <div class="navigatebar navigatebar-button navigatebar-mine">
		<a style="color:#304975" href="<%- url_for() %>"><%= config.title %></a>
	  </div>
	  <ul id="main-menu" class="main-menu">
	    <% if (page.links){ %>
          <% (page.links||[]).forEach(function(group){ %>
            <% if(group.items) { %>
		      <li>
				<a href="#<%- group.group %>" class="smooth">
				<i class="linecons-star"></i>
				<span class="title"><%- group.group %></span>
				<span class="label label-pink pull-right hidden-collapsed"><%- group.items.length%></span>
				</a>
		      </li>
		<% }})} %>
	  </ul>
	</div>
  </div>
  <div class="main-content">
    

    <%- page.excerpt %>
        <% if (page.links){ %>
          <% (page.links||[]).forEach(function(group){ %>
            <% if(group.items) { %>
                <% if(group.group) { %>
                  <h4 class="text-pink">
				    <i class="linecons-tag" style="margin-right: 7px;" id="<%= group.group %>"></i>
                    <%= group.group %>
                  </h4>
                <%} %>
				<% for(var i = 0; i < group.items.length; i+=4){ %>
				  <div class="row">
				    <div class="col-sm-3">
					  <div class="xe-widget xe-conversations box2 label-info" onclick="window.open('<%- url_for(group.items[i].url || '') %>', '_blank')" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="<%- url_for(group.items[i].url || '') %>">
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i].url || '') %>'>
						  <div class="xe-user-img">
						    <img class="img-circle" src="/img/friend.png" width="40">
						  </div>
						  <div class="xe-comment">
						    <strong><%- group.items[i].name %></strong>
							
						  </div>
						</a>
					  </div>
					</div>
					
					<% if(i+1<group.items.length){ %>
					<div class="col-sm-3">
					  <div class="xe-widget xe-conversations box2 label-info" onclick="window.open('<%- url_for(group.items[i+1].url || '') %>', '_blank')" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="<%- url_for(group.items[i+1].url || '') %>">
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i].url || '') %>'>
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i+1].url || '') %>'>
						  <div class="xe-user-img">
						    <img class="img-circle" src="/img/friend.png" width="40">
						  </div>
						  <div class="xe-comment">
						    <strong><%- group.items[i+1].name %></strong>
							<p class="overflowClip_2">
							  <a href='<%- url_for(group.items[i+1].url || '') %>'>
						        <%- group.items[i+1].desc %>
					          </a>
							</p>
						  </div>
						</a>
					  </div>
					</div>
					<% } %>
					<% if(i+2<group.items.length){ %>
					<div class="col-sm-3">
					  <div class="xe-widget xe-conversations box2 label-info" onclick="window.open('<%- url_for(group.items[i+2].url || '') %>', '_blank')" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="<%- url_for(group.items[i+2].url || '') %>">
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i].url || '') %>'>
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i+2].url || '') %>'>
						  <div class="xe-user-img">
						    <img class="img-circle" src="/img/friend.png" width="40">
						  </div>
						  <div class="xe-comment">
						    <strong><%- group.items[i+2].name %></strong>
							<p class="overflowClip_2">
							  <a href='<%- url_for(group.items[i+2].url || '') %>'>
						        <%- group.items[i+2].desc %>
					          </a>
							</p>
						  </div>
						</a>
					  </div>
					</div>
					<% } %>
					<% if(i+3<group.items.length){ %>
					<div class="col-sm-3">
					  <div class="xe-widget xe-conversations box2 label-info" onclick="window.open('<%- url_for(group.items[i+3].url || '') %>', '_blank')" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="<%- url_for(group.items[i+3].url || '') %>">
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i].url || '') %>'>
					    <a class="xe-comment-entry" href='<%- url_for(group.items[i+3].url || '') %>'>
						  <div class="xe-user-img">
						    <img class="img-circle" src="/img/friend.png" width="40">
						  </div>
						  <div class="xe-comment">
						    <strong><%- group.items[i+3].name %></strong>
							<p class="overflowClip_2">
							  <a href='<%- url_for(group.items[i+3].url || '') %>'>
						        <%- group.items[i+3].desc %>
					          </a>
							</p>
						  </div>
						</a>
					  </div>
					</div>
					<% } %>
				  </div>
                 <% } %>
            <%} %>
          <% }) %>
        <% } %>
    <%- page.more %>
  </div>
</div>
